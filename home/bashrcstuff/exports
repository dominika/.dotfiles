source "$DOT_FILES/configs/private"

export MANPAGER='col -bx | vim -c ":set ft=man nonu nolist" -R -'
export LANG=en_US.UTF-8
export LC_ALL=$LANG
export LS_OPTIONS='--color=auto'
export EDITOR='vim'
export GEM_EDITOR='vim'
#export CC=/usr/bin/gcc-4.2
export TERM="xterm-256color"

export OPSCODE_USER=maui

export RAILS_ENV=development

export GOPATH=~/go/bin

export PATH="/usr/local/bin:/usr/local/sbin:/usr/local/heroku/bin:/usr/local/opt/coreutils/libexec/gnubin:~/Library/Python/2.7/bin:$GOPATH:$PATH"

eval `dircolors ~/.dir_colors`

[ -f ~/.fzf.bash ] && source ~/.fzf.bash
